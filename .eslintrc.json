{
    "extends": "next/core-web-vitals",
    "plugins": ["simple-import-sort", "unused-imports"],
    "rules": {
        "react/no-children-prop": "off",
        "react/no-unescaped-entities": "off",
        "simple-import-sort/imports": "warn",
